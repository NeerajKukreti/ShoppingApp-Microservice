services:
  shopping.client:
    build:
      context: D:\Microservices\AKS\gitrepo\Shopping.Client
      dockerfile: D:\Microservices\AKS\gitrepo\Shopping.Client\Shopping.client\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Shopping.client
      target: base
    container_name: shoppingclient
    depends_on:
      shopping.api:
        condition: service_started
      shopping.db:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      ShoppingAPIUrl: http://shoppingapi
    image: shoppingclient:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/Shopping.client.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 8001
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Microservices\AKS\gitrepo\Shopping.Client\Shopping.client
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Microservices\AKS\gitrepo\Shopping.Client
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ASUS\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ASUS\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft\Xamarin\NuGet\
      target: /root/.nuget/fallbackpackages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ASUS\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ASUS\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  shopping.db:
    container_name: shoppingdb
    image: mongo
    networks:
      default: null
    ports:
    - mode: ingress
      target: 27017
      published: 27017
      protocol: tcp
    restart: always
    volumes:
    - type: bind
      source: /site
      target: /data/db
      bind:
        create_host_path: true
  shoppingapi:
    build:
      context: D:\Microservices\AKS\gitrepo\Shopping.Client
      dockerfile: D:\Microservices\AKS\gitrepo\Shopping.Client\ShoppingAPI\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ShoppingAPI
      target: base
    container_name: shoppingapi
    depends_on:
      shopping.db:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      Databasesetting:ConnectionString: mongodb://shoppingdb:27017
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: shoppingapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/ShoppingAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 8000
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Microservices\AKS\gitrepo\Shopping.Client\ShoppingAPI
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Microservices\AKS\gitrepo\Shopping.Client
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ASUS\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ASUS\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft\Xamarin\NuGet\
      target: /root/.nuget/fallbackpackages
